<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>TcWeb.service.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>TcWeb.service.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>mailboxVerification </servlet-name>
        <servlet-class>TcWeb.service.mailboxVerification</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mailboxVerification</servlet-name>
        <url-pattern>/mailboxVerification </url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>TcWeb.service.test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>10</session-timeout>
    </session-config>
    <servlet>
        <servlet-name>main</servlet-name>
        <servlet-class>TcWeb.service.main</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>main</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>content</servlet-name>
        <servlet-class>TcWeb.service.content</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>content</servlet-name>
        <url-pattern>/content</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>query</servlet-name>
        <servlet-class>TcWeb.service.query</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>query</servlet-name>
        <url-pattern>/query</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>writeBook</servlet-name>
        <servlet-class>TcWeb.service.writeBook</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>writeBook</servlet-name>
        <url-pattern>/writeBook</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>TcWeb.DataBase.UploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/UploadServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>write</servlet-name>
        <servlet-class>TcWeb.service.write</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>write</servlet-name>
        <url-pattern>/write</url-pattern>
    </servlet-mapping>
</web-app>